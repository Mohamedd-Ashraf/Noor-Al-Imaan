<?xml version="1.0" encoding="utf-8"?>
<!--
  Prevents the Android resource shrinker from removing adhan audio files.
  These files are referenced dynamically via resources.getIdentifier(soundName, "raw", packageName)
  which bypasses static analysis. Without this file, the shrinker removes adhan_2.mp3â€“adhan_10.mp3
  in release builds because they appear "unreferenced".
-->
<resources xmlns:tools="http://schemas.android.com/tools"
    tools:keep="@raw/adhan_1,@raw/wird_reminder,
               @raw/iqama_sound,@raw/iqama_sound_full,
               @raw/short_adhan,
               @raw/prayer_approaching_dhuhr,@raw/prayer_approaching_maghrib,
               @raw/prayer_reminder_fajr,@raw/prayer_reminder_dhuhr,
               @raw/prayer_reminder_asr,@raw/prayer_reminder_maghrib,@raw/prayer_reminder_isha,
               @raw/salawat_1,@raw/salawat_2,@raw/salawat_3,@raw/salawat_4,@raw/salawat_5" />
